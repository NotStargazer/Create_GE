{
  "multipart": [
    {
      "apply": {
        "model": "create:block/metal_girder_encased_shaft_tier_1/block"
      },
      "when": {
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "create:block/metal_girder_encased_shaft_tier_1/block",
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "create:block/metal_girder_encased_shaft_tier_1/block_top"
      },
      "when": {
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "create:block/metal_girder_encased_shaft_tier_1/block_bottom"
      },
      "when": {
        "bottom": "true"
      }
    }
  ]
}